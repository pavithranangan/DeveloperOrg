    <validationRules>
        <fullName>fully_paid</fullName>
        <active>true</active>
        <description>if the actual amount equal to amount to be paid, when fully paid option is chosed</description>
        <errorConditionFormula>ISPICKVAL( Status__c ,&quot;Fully paid&quot;)  &amp;&amp;  Actual_amount_paid__c != Amount_to_be_paid__c</errorConditionFormula>
        <errorMessage>Actual amount must need to be equal to full amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>not_paid</fullName>
        <active>true</active>
        <description>if the not paid option is clicked then not aloow to save</description>
        <errorConditionFormula>ISPICKVAL( Status__c ,&quot;Not paid&quot;)  &amp;&amp;  Actual_amount_paid__c != Amount_to_be_paid__c</errorConditionFormula>
        <errorMessage>Allowed to save when payment is fully paid</errorMessage>
    </validationRules>
