    <validationRules>
        <fullName>Floor_number</fullName>
        <active>true</active>
        <errorConditionFormula>Floor_number__c  &gt;=  Apartment_Name__r.Number_of_floors__c</errorConditionFormula>
        <errorMessage>Verify the total number of floors</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Salary</fullName>
        <active>true</active>
        <errorConditionFormula>Salary_per_annum__c  &lt; 3000000</errorConditionFormula>
        <errorMessage>Salary must need to be greater or equal to 30L</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stages_is_complesory</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Stages__c , &quot;&quot;)</errorConditionFormula>
        <errorMessage>knidly enter the valid stages</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>floor_number_need_to_match_apartment</fullName>
        <active>true</active>
        <errorConditionFormula>Floor_number__c  &gt; Apartment_Name__r.Number_of_floors__c</errorConditionFormula>
        <errorMessage>No such floor available, check apartment</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>have_own_house_enter_loan</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Have_own_house__c , &quot;Yes&quot;) &amp;&amp;  ISBLANK(  IF(ISPICKVAL( Do_have_loan__c , &quot;Yes&quot;),&quot;True&quot;,&quot;False&quot;) )</errorConditionFormula>
        <errorMessage>If have own house, then kindly enter the loan details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>if_own_house_enter_current_address</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Have_own_house__c , &quot;Yes&quot;) &amp;&amp;  ISBLANK( Current_Address__c )</errorConditionFormula>
        <errorMessage>If ypou have own house, kindly enter current address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>minimum_amount_for_penthouse</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.Name ==&quot;Penthouse&quot; &amp;&amp; Total_cost_with_discount__c &gt;60000000</errorConditionFormula>
        <errorMessage>Maximum amount for penthouse is 6C</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>stages_is_lost_reason_must</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Stages__c , &quot;Lost&quot;) &amp;&amp;  ISBLANK( Reason_for_lost__c )</errorConditionFormula>
        <errorMessage>If the enquiry is lost, then kindly select the reason.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
