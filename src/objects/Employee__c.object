    <validationRules>
        <fullName>department_should_not_contain_numbers</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Mobile_Number__c )</errorConditionFormula>
        <errorMessage>Mobile number is missing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>if_selected_then_DOJ_must</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Selected__c , &quot;Yes&quot;) &amp;&amp;  ISBLANK( Date_of_joining__c )</errorConditionFormula>
        <errorMessage>If you are selected, enter the joining date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>onbounding</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Selected__c , &quot;Yes&quot;) &amp;&amp;  ISPICKVAL( Onbounded__c , &quot;Yes&quot;)  &amp;&amp;  (TODAY() -  Date_of_joining__c &lt;0)</errorConditionFormula>
        <errorMessage>date of joining must need to be greater or equal to current date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>onbounding_says_no</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Selected__c , &quot;Yes&quot;) &amp;&amp; ISPICKVAL( Onbounded__c , &quot;No&quot;) &amp;&amp; ( TODAY() - Date_of_joining__c )&lt;=0 || ISPICKVAL( Selected__c , &quot;Yes&quot;) &amp;&amp; ISPICKVAL( Onbounded__c , &quot;No&quot;) &amp;&amp; ( TODAY() - Date_of_joining__c )&gt;0</errorConditionFormula>
        <errorMessage>Apply when you are joining</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>selected_is_no</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Selected__c , &quot;No&quot;) &amp;&amp;  ISPICKVAL( Onbounded__c , &quot;Yes&quot;) &amp;&amp; ( TODAY() - Date_of_joining__c ) &lt;=0 || ISPICKVAL( Selected__c , &quot;No&quot;) &amp;&amp;  ISPICKVAL( Onbounded__c , &quot;No&quot;) &amp;&amp; ( TODAY() - Date_of_joining__c ) &lt;=0 || ISPICKVAL( Selected__c , &quot;No&quot;) &amp;&amp;  ISPICKVAL( Onbounded__c , &quot;No&quot;) &amp;&amp; ( TODAY() - Date_of_joining__c ) &gt;0</errorConditionFormula>
        <errorMessage>if no selected then please don&apos;t apply</errorMessage>
    </validationRules>
